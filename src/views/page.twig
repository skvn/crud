    <script src="/vendor/src/bower_components/datatables/media/js/jquery.dataTables.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/datatables/media/js/dataTables.bootstrap.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/jquery-validation/dist/jquery.validate.js?s={{ config_get('app.serial') }}"></script>
