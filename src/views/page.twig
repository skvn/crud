    <script src="/vendor/src/bower_components/bootstrap/dist/js/bootstrap.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/bootstrap-combobox/js/bootstrap-combobox.js?s={{ config_get('app.serial') }}"></script>
