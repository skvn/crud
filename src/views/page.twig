    <script src="/vendor/src/bower_components/matchHeight/jquery.matchHeight.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/moment/min/moment-with-locales.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/nestable/jquery.nestable.js?s={{ config_get('app.serial') }}"></script>
