    <script src="/vendor/src/bower_components/jquery-form/jquery.form.js?s={{ config_get('app.serial') }}"></script>
    <script src="/vendor/src/bower_components/typeahead.js/dist/typeahead.bundle.js?s={{ config_get('app.serial') }}"></script>
